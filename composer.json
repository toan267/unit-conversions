{
    "name": "toan267/unitconversions",
    "description": "This is my package unitconversions",
    "keywords": [
        "toan267",
        "unitconversions"
    ],
    "homepage": "https://github.com/toan267/unitconversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Toan Fug",
            "email": "toan267@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UnitConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UnitConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {

            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
